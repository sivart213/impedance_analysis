{
    "model": "p(R1,C1)-p(R2,C2)-p(R3,C3)",
    "load_dir": {
        "__path__": "~/Documents"
    },
    "save_dir": {
        "__path__": "~/Documents"
    },
    "export_dir": {
        "__path__": "~/Documents"
    },
    "error_var": "Mean Abs Err",
    "sel_error_var": "Z",
    "afit_inputs": {
        "Match String": {
            "default": "fit",
            "tooltip": "A regex string to match datasets to fit."
        },
        "New Name Suffix": {
            "default": "fit",
            "tooltip": "The suffix to append to the dataset name (should not include number)."
        },
        "Fit Type": {
            "default": "Normal",
            "tooltip": "The type of fit to perform.",
            "widget": "QComboBox",
            "limits": [
                "Normal",
                "Iterative",
                "Bootstrap"
            ]
        },
        "Iterations": {
            "default": 10,
            "tooltip": "The number of iterations to perform for Iterative or Bootstrap fitting.",
            "widget": "QSpinBox",
            "limits": [
                1,
                100000,
                1
            ]
        },
        "Fit All Datasets": {
            "default": false,
            "tooltip": "Forces a fit of all datasets. Ignores pinned data and uses the current model.",
            "widget": "QCheckBox"
        },
        "Use Current Model": {
            "default": false,
            "tooltip": "Whether to use the current model or the pin-specific models.",
            "widget": "QCheckBox"
        },
        "Use Locked Parameter Value": {
            "default": false,
            "tooltip": "Whether to use the parameter value or the pinned value (if available) as the constant.",
            "widget": "QCheckBox"
        },
        "Respect Bound Locks": {
            "default": false,
            "tooltip": "Whether to shift locked bounds for new initial guess conflicts.",
            "widget": "QCheckBox"
        },
        "Fit Parameters Separately": {
            "default": false,
            "tooltip": "Whether to fit each parameter separately (ie sequentially).",
            "widget": "QCheckBox"
        },
        "Value Override": {
            "default": "",
            "tooltip": "Redefine any parameters (in dict notation) to be applied to all fits. Note that this only changes the start point."
        }
    },
    "plot_var": {
        "ny_x": "Z'",
        "ny_y": "-Z''",
        "bd_x": "Frequency",
        "bd_y1": "Z'",
        "bd_y2": "Z''"
    },
    "is_log": {
        "ny_x": false,
        "ny_y": false,
        "bd_x": true,
        "bd_y1": false,
        "bd_y2": false
    },
    "var_units": {
        "f": "Hz",
        "\u03C9": "rad/s",
        "\u03c3_dc": "S/cm",
        "Z": "\u03A9",
        "Y": "S",
        "M": "cm/F",
        "C": "F",
        "\u03b5\u1d63": "1",
        "\u03b5\u1d63_dc": "1",
        "\u03c3": "S/cm",
        "\u03c1": "\u03A9 cm",
        "\u03c3_dc": "S/cm"
    },
    "option_inits": {
        "general": {
            "additional_save_forms": "e_r",
            "save_current_parameters": true,
            "save_current_profile": true,
            "save_pinned_results": true,
            "include_unfit_datasets": false,
            "error_forms": [
                "Z.real",
                "Z.imag"
            ],
            "backup pins": true,
            "number of backups": 10,
            "backup path ('~' == home dir)": "~/Documents/z_fit/backups",
            "limit_error": true,
            "debounce_interval": 2000
        },
        "plotting": {
            "style": "seaborn-v0_8-darkgrid",
            "background": "",
            "always plot to fullrange": false,
            "allow inv log": false,
            "suplots: use numbers": false,
            "suplots: shift by real": true,
            "suplots: shift using max": true,
            "window padding": 0.2,
            "outlier percent": 0,
            "inversion threshold": 0.95,
            "plot1 as nyquist": true,
            "Data options": {
                "s": 30,
                "alpha": 1,
                "zorder": 1,
                "a_kwargs": {
                    "mag_off": 0.05
                }
            },
            "Model options": {
                "color": "red",
                "alpha": 0.95,
                "zorder": 2
            },
            "Sub Model options": {
                "cmap": "autumn",
                "linestyle": "--",
                "alpha": 0.95,
                "zorder": 2
            },
            "connect points": {
                "show": true,
                "color": "grey",
                "alpha": 0.5,
                "linestyle": "--",
                "linewidth": 0.75,
                "zorder": 0
            }
        },
        "simulation": {
            "sim_param_freq": true,
            "interp": true,
            "freq_start": -4.5,
            "freq_stop": 7,
            "freq_num": 200,
            "area": 25,
            "thickness": 0.045,
            "savgol_kwargs": {
                "window_length": 5,
                "polyorder": 3
            },
            "interp_kwargs": {
                "old_x": "freq"
            },
            "norm_kwargs": {
                "normalize_to":  "f@10**6",
                "x_array": "freq"
            }
        },
        "bands": {
            "band_color": "gray",
            "band_alpha": 0.2,
            "band_freq_num": 250,
            "band_mult": 50,
            "percentile": 5,
            "std_devs": 0.2,
            "conf_band_upper": "97.5%",
            "conf_band_lower": "2.5%"
        },
        "fit": {
            "function": "least_squares",
            "type": "impedance",
            "modes": ["real", "imag", "mag"],
            "Rs": "",
            "f_max": 1e7,
            "f_min": 1e-6,
            "scale": "linear",
            "weight_by_mode": "mag",
            "bootstrap_percent": 95,
            "prioritize_bounds": false,
            "rebound_before_fit": false,
            "resort_by_RC": false,
            "sequential_fit": false,
            "sequential_fit_update": false
        },
        "curve_fit": {
            "absolute_sigma": false,
            "check_finite": null,
            "method": null,
            "jac": "3-point",
            "x_scale": "jac",
            "ftol": 1e-14,
            "xtol": 1e-6,
            "gtol": 1e-8,
            "loss": "cauchy",
            "diff_step": null,
            "tr_solver": null,
            "tr_options": {},
            "jac_sparsity": null,
            "verbose": 0,
            "maxfev": 1000000
        },
        "least_sq": {
            "method": "trf",
            "jac": "3-point",
            "x_scale": "calc",
            "ftol": 1e-14,
            "xtol": 1e-15,
            "gtol": 1e-8,
            "loss": "cauchy",
            "diff_step": null,
            "tr_solver": null,
            "tr_options": {},
            "jac_sparsity": null,
            "verbose": 0,
            "max_nfev": 1000
        },
        "diff_evolution": {
            "strategy": "best1bin",
            "maxiter": 1000,
            "popsize": 15,
            "tol": 0.01,
            "recombination": 0.7,
            "seed": null,
            "disp": false,
            "polish": true,
            "init": "latinhypercube",
            "atol": 0,
            "updating": "immediate",
            "workers": 1
        },
        "basinhopping": {
            "niter": 100,
            "T": 1.0,
            "stepsize": 0.5,
            "minimizer_kwargs": {
                "method": "L-BFGS-B",
                "jac": "3-point"
            },
            "interval": 50,
            "disp": false,
            "niter_success": null,
            "seed": null,
            "target_accept_rate": 0.5,
            "stepwise_factor": 0.9
        },
        "linkk": {
            "f_max": 1000000.0,
            "f_min": 0.000001,
            "c": 0.5,
            "max_M": 200,
            "add_cap": false,
            "fit_type": "complex"
        },
        "element": {
            "R": 1000000000.0,
            "C": 0.0000000001,
            "CPE": [0.0000000001, 1],
            "ICPE": [1000000000.0, 0],
            "L": 100,
            "W": 10000,
            "K": [1000000000.0, 1]
        },
        "element_range": {
            "R": [
                0,
                {
                    "__invalid_float__": "inf"
                }
            ],
            "C": [
                0,
                1
            ],
            "CPE": [
                [
                    0,
                    1
                ],
                [
                    -1,
                    1
                ]
            ],
            "ICPE": [
                [
                    0,
                    {
                    "__invalid_float__": "inf"
                }
                ],
                [
                    -1,
                    1
                ]
            ],
            "L": [
                0,
                {
                    "__invalid_float__": "inf"
                }
            ],
            "W": [
                0,
                {
                    "__invalid_float__": "inf"
                }
            ],
            "K": [
                [
                    0,
                    {
                    "__invalid_float__": "inf"
                }
                ],
                [
                    0,
                    {
                    "__invalid_float__": "inf"
                }
                ]
            ]
        }
    }
}